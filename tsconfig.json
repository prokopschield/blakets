{
	"compilerOptions": {
		"target": "ES2018",
		"module": "CommonJS",
		"declaration": true,
		"outDir": "./lib",
		"esModuleInterop": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"noUnusedParameters": true,
		"noUnusedLocals": true,
		"noImplicitAny": true
	},
	"include": ["src/**/*"],
	"exclude": []
}
